{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "api/main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/main.go",
      "check": true
    },
    {
      "src": "/(.*)",
      "dest": "/my-react-app/$1"
    }
  ]
}
